{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "ScriptedBash",
	"patterns": [
		{
		  "match": "\\b(shabang|define|if|else if|println|run|srun|end)\\b",
		  "name": "keyword.control.scriptedbash"
		},
		{
		  "match": "\"[^\"]*\"",
		  "name": "string.quoted.double.scriptedbash"
		}
	],
	"scopeName": "source.scriptedbash"
}